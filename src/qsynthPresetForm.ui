<author>rncbc aka Rui Nuno Capela</author>
<widget class="QDialog">
    <property name="name">
        <cstring>qsynthPresetForm</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>402</width>
            <height>350</height>
        </rect>
    </property>
    <property name="caption">
        <string>Channel Preset</string>
    </property>
    <property name="icon">
        <pixmap>channels1.png</pixmap>
    </property>
    <grid>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <widget class="QLayoutWidget" row="1" column="0">
            <property name="name">
                <cstring>layout2</cstring>
            </property>
            <hbox>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <spacer>
                    <property name="name">
                        <cstring>spacer2</cstring>
                    </property>
                    <property name="orientation">
                        <enum>Horizontal</enum>
                    </property>
                    <property name="sizeType">
                        <enum>Expanding</enum>
                    </property>
                    <property name="sizeHint">
                        <size>
                            <width>120</width>
                            <height>8</height>
                        </size>
                    </property>
                </spacer>
                <widget class="QPushButton">
                    <property name="name">
                        <cstring>OkPushButton</cstring>
                    </property>
                    <property name="text">
                        <string>OK</string>
                    </property>
                    <property name="default">
                        <bool>true</bool>
                    </property>
                    <property name="iconSet">
                        <iconset>accept1.png</iconset>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string></string>
                    </property>
                </widget>
                <widget class="QPushButton">
                    <property name="name">
                        <cstring>CancelPushButton</cstring>
                    </property>
                    <property name="text">
                        <string>Cancel</string>
                    </property>
                    <property name="iconSet">
                        <iconset>quit1.png</iconset>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string></string>
                    </property>
                </widget>
            </hbox>
        </widget>
        <widget class="QTabWidget" row="0" column="0">
            <property name="name">
                <cstring>PresetTabWidget</cstring>
            </property>
            <property name="tabPosition">
                <enum>Top</enum>
            </property>
            <property name="tabShape">
                <enum>Rounded</enum>
            </property>
            <widget class="QWidget">
                <property name="name">
                    <cstring>tab</cstring>
                </property>
                <attribute name="title">
                    <string>Preset</string>
                </attribute>
                <grid>
                    <property name="name">
                        <cstring>unnamed</cstring>
                    </property>
                    <widget class="QComboBox" row="1" column="0" rowspan="1" colspan="2">
                        <property name="name">
                            <cstring>SoundFontComboBox</cstring>
                        </property>
                        <property name="toolTip" stdset="0">
                            <string>Soundfont selector</string>
                        </property>
                    </widget>
                    <widget class="QLabel" row="0" column="0" rowspan="1" colspan="2">
                        <property name="name">
                            <cstring>SoundFontTextLabel</cstring>
                        </property>
                        <property name="text">
                            <string>&amp;Soundfont:</string>
                        </property>
                        <property name="buddy" stdset="0">
                            <cstring>SoundFontComboBox</cstring>
                        </property>
                    </widget>
                    <widget class="QListBox" row="3" column="0">
                        <property name="name">
                            <cstring>BankListBox</cstring>
                        </property>
                        <property name="sizePolicy">
                            <sizepolicy>
                                <hsizetype>1</hsizetype>
                                <vsizetype>7</vsizetype>
                                <horstretch>0</horstretch>
                                <verstretch>0</verstretch>
                            </sizepolicy>
                        </property>
                        <property name="maximumSize">
                            <size>
                                <width>80</width>
                                <height>32767</height>
                            </size>
                        </property>
                        <property name="toolTip" stdset="0">
                            <string>Bank selector</string>
                        </property>
                    </widget>
                    <widget class="QListBox" row="3" column="1">
                        <property name="name">
                            <cstring>ProgramListBox</cstring>
                        </property>
                        <property name="toolTip" stdset="0">
                            <string>Program selector</string>
                        </property>
                    </widget>
                    <widget class="QLabel" row="2" column="1">
                        <property name="name">
                            <cstring>ProgramTextLabel</cstring>
                        </property>
                        <property name="text">
                            <string>&amp;Program:</string>
                        </property>
                        <property name="buddy" stdset="0">
                            <cstring>ProgramListBox</cstring>
                        </property>
                    </widget>
                    <widget class="QLabel" row="2" column="0">
                        <property name="name">
                            <cstring>BankTextLabel</cstring>
                        </property>
                        <property name="text">
                            <string>&amp;Bank:</string>
                        </property>
                        <property name="buddy" stdset="0">
                            <cstring>BankListBox</cstring>
                        </property>
                    </widget>
                </grid>
            </widget>
        </widget>
    </grid>
</widget>
<connections>
    <connection>
        <sender>SoundFontComboBox</sender>
        <signal>highlighted(int)</signal>
        <receiver>qsynthPresetForm</receiver>
        <slot>soundfontChanged()</slot>
    </connection>
    <connection>
        <sender>BankListBox</sender>
        <signal>highlighted(int)</signal>
        <receiver>qsynthPresetForm</receiver>
        <slot>bankChanged()</slot>
    </connection>
    <connection>
        <sender>ProgramListBox</sender>
        <signal>selected(int)</signal>
        <receiver>qsynthPresetForm</receiver>
        <slot>acceptForm()</slot>
    </connection>
    <connection>
        <sender>OkPushButton</sender>
        <signal>clicked()</signal>
        <receiver>qsynthPresetForm</receiver>
        <slot>acceptForm()</slot>
    </connection>
    <connection>
        <sender>CancelPushButton</sender>
        <signal>clicked()</signal>
        <receiver>qsynthPresetForm</receiver>
        <slot>reject()</slot>
    </connection>
</connections>
<tabstops>
    <tabstop>PresetTabWidget</tabstop>
    <tabstop>SoundFontComboBox</tabstop>
    <tabstop>BankListBox</tabstop>
    <tabstop>ProgramListBox</tabstop>
    <tabstop>OkPushButton</tabstop>
    <tabstop>CancelPushButton</tabstop>
</tabstops>
<includes>
    <include location="global" impldecl="in declaration">qsynthSetup.h</include>
    <include location="local" impldecl="in implementation">qsynthPresetForm.ui.h</include>
</includes>
<variables>
    <variable access="private">fluid_synth_t *m_pSynth;</variable>
    <variable access="private">int m_iChan;</variable>
    <variable access="private">fluid_preset_t *m_pPreset;</variable>
</variables>
<slots>
    <slot specifier="non virtual">stabilizeForm()</slot>
    <slot specifier="non virtual">acceptForm()</slot>
    <slot specifier="non virtual">soundfontChanged()</slot>
    <slot specifier="non virtual">bankChanged()</slot>
</slots>
<functions>
    <function access="private" specifier="non virtual">init()</function>
    <function access="private" specifier="non virtual">destroy()</function>
    <function specifier="non virtual">setup( fluid_synth_t *pSynth, int iChan )</function>
    <function access="private" specifier="non virtual" returnType="bool">validateForm()</function>
    <function access="private" specifier="non virtual" returnType="int">findSoundFontIndex(int iSFID)</function>
    <function access="private" specifier="non virtual" returnType="int">findBankIndex(int iBank)</function>
    <function access="private" specifier="non virtual" returnType="int">findProgramIndex(int iProgram)</function>
</functions>
<pixmapinproject/>
<layoutdefaults spacing="4" margin="8"/>
</UI>
