<author>rncbc aka Rui Nuno Capela</author>
<widget class="QWidget">
    <property name="name">
        <cstring>qsynthMessagesForm</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>529</width>
            <height>210</height>
        </rect>
    </property>
    <property name="sizePolicy">
        <sizepolicy>
            <hsizetype>1</hsizetype>
            <vsizetype>1</vsizetype>
            <horstretch>0</horstretch>
            <verstretch>0</verstretch>
        </sizepolicy>
    </property>
    <property name="font">
        <font>
        </font>
    </property>
    <property name="caption">
        <string>qsynth: Messages</string>
    </property>
    <property name="icon">
        <pixmap>qsynth.png</pixmap>
    </property>
    <grid>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <widget class="QTextEdit" row="0" column="0">
            <property name="name">
                <cstring>MessagesTextView</cstring>
            </property>
            <property name="minimumSize">
                <size>
                    <width>320</width>
                    <height>80</height>
                </size>
            </property>
            <property name="font">
                <font>
                    <family>Fixed</family>
                </font>
            </property>
            <property name="wordWrap">
                <enum>NoWrap</enum>
            </property>
            <property name="readOnly">
                <bool>true</bool>
            </property>
            <property name="undoRedoEnabled">
                <bool>false</bool>
            </property>
            <property name="toolTip" stdset="0">
                <string>Messages output log</string>
            </property>
        </widget>
    </grid>
</widget>
<tabstops>
    <tabstop>MessagesTextView</tabstop>
</tabstops>
<includes>
    <include location="local" impldecl="in implementation">qsynthMainForm.h</include>
    <include location="local" impldecl="in implementation">qsynthMessagesForm.ui.h</include>
</includes>
<variables>
    <variable access="private">int m_iMessagesLimit;</variable>
</variables>
<functions>
    <function access="private" specifier="non virtual">init()</function>
    <function access="private" specifier="non virtual">destroy()</function>
    <function access="private" specifier="non virtual">showEvent( QShowEvent * pShowEvent )</function>
    <function access="private" specifier="non virtual">hideEvent( QHideEvent * pHideEvent )</function>
    <function specifier="non virtual" returnType="QFont">messagesFont()</function>
    <function specifier="non virtual">setMessagesFont( const QFont &amp; font )</function>
    <function specifier="non virtual" returnType="int">messagesLimit()</function>
    <function specifier="non virtual">setMessagesLimit( int iLimit )</function>
    <function specifier="non virtual">appendMessages( const QString &amp; s )</function>
    <function specifier="non virtual">appendMessagesColor( const QString &amp; s, const QString &amp; c )</function>
    <function specifier="non virtual">appendMessagesText( const QString &amp; s )</function>
</functions>
<pixmapinproject/>
<layoutdefaults spacing="4" margin="4"/>
</UI>
